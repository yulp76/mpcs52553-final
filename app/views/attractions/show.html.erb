<% attraction = Attraction.find_by(id: params["id"]) %>
<% reviews = attraction.reviews %>
<% user_id = session[:user_id] %>
<% wish = Wish.find_by(user_id: user_id, attraction_id: attraction.id) %>

<%= render 'attraction_card', reviews: reviews, attraction: attraction, user_id: user_id, wish: wish %>
<%= render 'comment', reviews: reviews, attraction: attraction, user_id: user_id %>
